<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="citaapp.citapprenticeshipapp">


    <!-- Internet Permissions -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TimelineMain" />
        <activity android:name=".ApprenticeshipBegin" />
        <activity android:name=".ApprenticeshipEmployment" />
        <activity android:name=".ContractSignContract" />
        <activity android:name=".ContractApproval" />
        <activity android:name=".EnrolmentUSI" />
        <activity android:name=".EnrolmentTrainingPlan" />
        <activity android:name=".EnrolmentComplete" />
        <activity android:name=".EditInformation" />
        <activity android:name=".ViewInformation" />
        <activity android:name=".TestDatabaseConnection"/>
    </application>

</manifest>